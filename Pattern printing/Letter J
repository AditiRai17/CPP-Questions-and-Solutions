//Write a program to print letter J exactly as shown
#include <iostream>

using namespace std;

int main()
{
    int x , y ;
    for(x=0 ; x<=8 ; x++)
    {
        for(y=0 ; y<=11 ; y++)
        {
            if((y!=5&&y!=6)&&x==0)
            {
                cout << "$";
            }
            else if(((y==5||y==6)&&x!=8)||(x==6&&(y==0||y==1))||(x==7&&(y==1||y==2))||(x==8&&(y==3||y==4)))
            {
                cout << "#";
            }
            else
            {
                cout << " ";
            }
         }
         cout << "\n";
     }
     return 0;
}
